{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "development",
        "API_URL": "http://192.168.3.10:3000",
        "DEBUG_MODE": "true"
      }
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "staging",
        "API_URL": "https://staging-api.personaarcana.com",
        "DEBUG_MODE": "false"
      }
    },
    "production": {
      "env": {
        "ENVIRONMENT": "production",
        "API_URL": "https://api.personaarcana.com",
        "DEBUG_MODE": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}